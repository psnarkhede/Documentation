{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/debug",
            "dest": "/"
        },
        {
            "src": "/health",
            "dest": "/"
        },
        {
            "src": "/parse",
            "dest": "/"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "index.js": {
            "maxDuration": 30
        }
    }
}